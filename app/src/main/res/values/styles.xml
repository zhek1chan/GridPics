<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="settings_background">
        <item name="android:background">@color/white</item>
    </style>
    <style name="settings_text_view">
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="settings_image_view">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="tint">@color/black</item>
    </style>
    <style name="settings_back_icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="tint">@color/black</item>
    </style>

    <style name="settings_linear_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingVertical">8dp</item>
    </style>*--
</resources>